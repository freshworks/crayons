{
  "name": "freshworks-ui-kit",
  "version": "0.0.1",
  "description": "Freshworks UI Kit for Developer Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/freshworks-ui-kit/freshworks-ui-kit.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs && yarn storybook:build",
    "code-sanity": "yarn lint",
    "lint": "yarn lint:ts && yarn lint:sass",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -t grouped -s node_modules/custom-tslint-formatters/formatters",
    "lint:sass": "stylelint 'src/**/*.scss'",
    "start": "stencil build --docs --dev --watch",
    "test": "stencil test --spec --e2e --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "plop",
    "story": "npm-run-all --parallel start storybook",
    "story:test": "npm run test:generate && npm run story",
    "storybook": "start-storybook -p 9001 -c .storybook -s www",
    "storybook:build": "build-storybook -c .storybook -o storybook-dist",
    "unit-tests": "npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@stencil/core": "^1.5.1",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.0.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-events": "^5.2.8",
    "@storybook/addon-jest": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/html": "^5.2.1",
    "@storybook/preset-typescript": "^1.1.0",
    "@types/autoprefixer": "^9.6.1",
    "@types/jest": "^24.0.23",
    "@types/puppeteer": "^1.19.1",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.1",
    "custom-tslint-formatters": "^2.4.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.4.0",
    "puppeteer": "1.20.0",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "tslint-ionic-rules": "^0.0.21",
    "typescript": "^3.6.4",
    "write-file-webpack-plugin": "^4.5.1",
    "react-dom": "^16.12.0"
  },
  "license": "MIT"
}
